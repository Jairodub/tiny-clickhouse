<clickhouse>
    <users>
        <default>
            <!-- To use a plaintext password (NOT RECOMMENDED for production): -->
            <!-- <password>changeme</password> -->

            <!-- Using SHA256 hex is recommended. 
                 To generate: echo -n "mypassword" | sha256sum
            -->
            <password remove="1" />
            <password_sha256_hex>REPLACE_WITH_SHA256_HEX</password_sha256_hex>
        </default>
    </users>
</clickhouse>
